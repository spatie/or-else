{
    "name": "spatie/or-else",
    "description": "A trait to add orElse functions to a class",
    "keywords": [
        "spatie",
        "or-else",
        "optional"
    ],
    "homepage": "https://github.com/spatie/or-else",
    "license": "MIT",
    "authors": [
        {
            "name": ":Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.8 || ^5.7 || ^6.5",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\OrElse\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\OrElse\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
